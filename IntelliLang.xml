<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="JAVA" injector-id="xml">
      <display-name>StringConverter/@value</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("value")).withNamespace(string().equalTo("http://javafx.com/fxml/1")).withParent(xmlTag().withLocalName(string().equalTo("StringConverter")).withNamespace(string().equalTo("http://javafx.com/fxml")))]]></place>
    </injection>
  </component>
</application>