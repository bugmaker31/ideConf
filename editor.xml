<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="13" />
    <option name="FONT_SCALE" value="1.0833334" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="JAVA" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="REFRAIN_FROM_SCROLLING" value="true" />
  </component>
  <component name="InspectionProfileManager">
    <info color="ff4005">
      <option name="FOREGROUND" value="ffffff" />
      <option name="BACKGROUND" value="ff4005" />
      <option name="ERROR_STRIPE_COLOR" value="ff4005" />
      <option name="myName" value="Alert" />
      <option name="myVal" value="50" />
      <option name="myExternalName" value="Alert" />
      <option name="myDefaultAttributes">
        <option name="ERROR_STRIPE_COLOR" value="ff4005" />
      </option>
    </info>
    <list size="8">
      <item index="0" class="java.lang.String" itemvalue="INFORMATION" />
      <item index="1" class="java.lang.String" itemvalue="TYPO" />
      <item index="2" class="java.lang.String" itemvalue="SERVER PROBLEM" />
      <item index="3" class="java.lang.String" itemvalue="WEAK WARNING" />
      <item index="4" class="java.lang.String" itemvalue="INFO" />
      <item index="5" class="java.lang.String" itemvalue="WARNING" />
      <item index="6" class="java.lang.String" itemvalue="Alert" />
      <item index="7" class="java.lang.String" itemvalue="ERROR" />
    </list>
  </component>
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern icon="important" useCustomColors="true" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="ff6e11" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
    </pattern>
    <filter name="TODOs">
      <pattern index="0" />
    </filter>
    <filter name="FIXMEs">
      <pattern index="1" />
    </filter>
  </component>
</application>